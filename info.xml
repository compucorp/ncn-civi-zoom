<?xml version="1.1"?>
<extension key="ncn-civi-zoom" type="module">
  <file>ncn_civi_zoom</file>
  <name>NCN Civirules Actions for Zoom</name>
  <description>Provides Zoom integration for CiviCRM (Symbiotic fork)</description>
  <maintainer>
    <author>Mathieu Lu</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <releaseDate>2023-09-05</releaseDate>
  <version>3.0</version>
  <requires>
    <ext>org.civicoop.civirules</ext>
    <ext>oauth-client</ext>
  </requires>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.60</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/NcnCiviZoom</namespace>
  </civix>
</extension>
