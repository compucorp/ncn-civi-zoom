<?xml version="1.0"?>
<extension key="ncn-civi-zoom" type="module">
  <file>ncn_civi_zoom</file>
  <name>NCN Civirules Actions for Zoom</name>
  <description>Provides Zoom integration for CiviCRM (Symbiotic fork)</description>
  <maintainer>
    <author>Mathieu Lu</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <releaseDate>2024-11-29</releaseDate>
  <version>3.0</version>
  <requires>
    <ext>org.civicoop.civirules</ext>
    <ext>oauth-client</ext>
  </requires>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.60</ver>
  </compatibility>
  <comments/>
  <comments>PATCH 3</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/NcnCiviZoom</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <upgrader>CRM_NcnCiviZoom_Upgrader</upgrader>
</extension>
